pipeline{
	agent any
    stages{
	    stage('Code Scanning'){
		    steps{
		    	echo "I'm scanning the code..."
		    }
	    }
        stage('Build'){
            steps{
		echo "This the Build stage"
            }
        }
        stage('Test'){
            steps{
		echo "This is the Testing stage"
            }

        }
    }
}
